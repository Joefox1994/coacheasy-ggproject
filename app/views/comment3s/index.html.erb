<h1>Listing comment3s</h1>

<table>
  <tr>
    <th>Program</th>
    <th>Body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @comment3s.each do |comment3| %>
  <tr>
    <td><%= comment3.program_id %></td>
    <td><%= comment3.body %></td>
    <td><%= link_to 'Show', comment3 %></td>
    <td><%= link_to 'Edit', edit_comment3_path(comment3) %></td>
    <td><%= link_to 'Destroy', comment3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

